# test of overlord toml file

board = "Myir-FZ3"

[[includes]]
  resource = "FullZynqPs8"

[[instance]]
  name = "blinky"
  type = "gateware.blinky"

[[instance]]
  name = "litedram"
  type = "gateware.litedram"

[[connection]]
  type = "port"
  connection = "blinky.q -> LED.SYS_STATUS"

[[connection]]
  type = "clock"
  connection = "ref.clk.25MHz -> blinky.clk"

[[connection]]
  type = "constant"
  connection = "25_000_000 -> blinky.clk_freq_hz"

